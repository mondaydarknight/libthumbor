{
    "name": "mamilove/phumbor",
    "description": "A minimal PHP client for generating thumbor URLs",
    "type": "library",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thumbor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Thumbor": "Thumbor\\Facades\\Thumbor"
            },
            "providers": [
                "Thumbor\\ThumborServiceProvider"
            ]
        }
    }
}
